@model CV

@{
    ViewBag.Title = "Curriculum Vitae";
}

<h2>Curriculum Vitae</h2>

<h2>Curriculum Vitae</h2>

<table class="table">
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Gender</th>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <th>Date of Birth</th>
        <td>@Model.DateOfBirth.ToShortDateString()</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Experience</th>
        <td>@Model.Experience</td>
    </tr>
    <tr>
        <th>Skills</th>
        <td>@Model.Skills</td>
    </tr>
</table>
